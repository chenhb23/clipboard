{
  "name": "clipboard",
  "version": "1.0.0",
  "description": "macos 粘贴板应用",
  "main": "build/index.js",
  "scripts": {
    "dev": "parcel --target electron --public-url . public/index.html public/search.html",
    "start": "tsc && electron ."
  },
  "keywords": ["paste", "clipboard", "macos"],
  "author": "chenhb <chenhongbo_1994@163.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "since 2017-06"
  ],
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "electron": "^12.0.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
